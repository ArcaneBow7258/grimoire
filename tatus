[32m+[m[32m<script>[m
[32m+[m[41m  [m
[32m+[m[41m  [m
[32m+[m[32m  (function() {[m
[32m+[m[32m    const enableTocScroll = '1' == '1'[m
[32m+[m[32m    if (!enableTocScroll) {[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m[32m    if (!document.querySelector('.toc')) {[m
[32m+[m[32m      console.log('no toc found, ignore toc scroll')[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m    const scrollListeners = window.scrollListeners[m
[32m+[m[32m    const headings = document.querySelectorAll('h1[id],h2[id],h3[id],h4[id],h5[id]');[m
[32m+[m[32m    const activeClass = 'active';[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m    let activeHeading = headings[0];[m
[32m+[m[32m    getLinkByHeading(activeHeading).classList.add(activeClass);[m
[32m+[m
[32m+[m[32m    const onScroll = () => {[m
[32m+[m[32m      const passedHeadings = [];[m
[32m+[m[32m      for (const h of headings) {[m
[32m+[m[41m        [m
[32m+[m[32m        if (getOffsetTop(h) < 5) {[m
[32m+[m[32m          passedHeadings.push(h)[m
[32m+[m[32m        } else {[m
[32m+[m[32m          break;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      if (passedHeadings.length > 0) {[m
[32m+[m[32m        newActiveHeading = passedHeadings[passedHeadings.length - 1];[m
[32m+[m[32m      } else {[m
[32m+[m[32m        newActiveHeading = headings[0];[m
[32m+[m[32m      }[m
[32m+[m[32m      if (activeHeading != newActiveHeading) {[m
[32m+[m[32m        getLinkByHeading(activeHeading).classList.remove(activeClass);[m
[32m+[m[32m        activeHeading = newActiveHeading;[m
[32m+[m[32m        getLinkByHeading(activeHeading).classList.add(activeClass);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    let timer = null;[m
[32m+[m[32m    const scrollListener = () => {[m
[32m+[m[32m      if (timer !== null) {[m
[32m+[m[32m        clearTimeout(timer)[m
[32m+[m[32m      }[m
[32m+[m[32m      timer = setTimeout(onScroll, 50)[m
[32m+[m[32m    }[m
[32m+[m[32m    window.addEventListener('scroll', scrollListener, false);[m
[32m+[m[32m    scrollListeners.push(scrollListener)[m
[32m+[m
[32m+[m[32m    function getLinkByHeading(heading) {[m
[32m+[m[32m      const id = encodeURI(heading.getAttribute('id')).toLowerCase();[m
[32m+[m[32m      return document.querySelector(`.toc ul li a[href="#${id}"]`);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function getOffsetTop(heading) {[m
[32m+[m[32m      if (!heading.getClientRects().length) {[m
[32m+[m[32m        return 0;[m
[32m+[m[32m      }[m
[32m+[m[32m      let rect = heading.getBoundingClientRect();[m
[32m+[m[32m      return rect.top[m
[32m+[m[32m    }[m
[32m+[m[32m  })();[m
[32m+[m[32m  </script>[m

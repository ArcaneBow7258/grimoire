[32m+[m[32mfunction switchTheme(theme) {[m
[32m+[m[32m  switch (theme) {[m
[32m+[m[32m    case 'light':[m
[32m+[m[32m      document.body.classList.remove('dark');[m
[32m+[m[32m      break;[m
[32m+[m[32m    case 'dark':[m
[32m+[m[32m      document.body.classList.add('dark');[m
[32m+[m[32m      break;[m
[32m+[m[41m    [m
[32m+[m[32m    default:[m
[32m+[m[32m      if (window.matchMedia('(prefers-color-scheme: dark)').matches) {[m
[32m+[m[32m        document.body.classList.add('dark');[m
[32m+[m[32m      }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isDarkTheme() {[m
[32m+[m[32m  return document.body.className.includes("dark");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getPrefTheme() {[m
[32m+[m[32m  return localStorage.getItem("pref-theme");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction setPrefTheme(theme) {[m
[32m+[m[32m  switchTheme(theme)[m
[32m+[m[32m  localStorage.setItem("pref-theme", theme);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst toggleThemeCallbacks = {}[m
[32m+[m[32mtoggleThemeCallbacks['main'] = (isDark) => {[m
[32m+[m[41m  [m
[32m+[m[32m  if (isDark) {[m
[32m+[m[32m    setPrefTheme('light');[m
[32m+[m[32m  } else {[m
[32m+[m[32m    setPrefTheme('dark');[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mwindow.addEventListener('toggle-theme', function() {[m
[32m+[m[41m  [m
[32m+[m[32m  const isDark = isDarkTheme()[m
[32m+[m[32m  for (const key in toggleThemeCallbacks) {[m
[32m+[m[32m    toggleThemeCallbacks[key](isDark)[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m
[32m+[m[32mfunction toggleThemeListener() {[m
[32m+[m[41m  [m
[32m+[m[32m  window.dispatchEvent(new CustomEvent('toggle-theme'));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m</script>[m
[32m+[m[32m<script>[m
[32m+[m[41m  [m
[32m+[m[32m  (function() {[m
[32m+[m[32m    const defaultTheme = 'dark';[m
[32m+[m[32m    const prefTheme = getPrefTheme();[m
[32m+[m[32m    const theme = prefTheme ? prefTheme : defaultTheme;[m
[32m+[m
[32m+[m[32m    switchTheme(theme);[m
[32m+[m[32m  })();[m
[32m+[m[32m</script>[m

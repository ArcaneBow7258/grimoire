[32m+[m[32m<script>[m
[32m+[m[32m  (function () {[m
[32m+[m[32m    let menu = document.getElementById('menu')[m
[32m+[m[32m    if (menu) {[m
[32m+[m[32m      menu.scrollLeft = localStorage.getItem("menu-scroll-position");[m

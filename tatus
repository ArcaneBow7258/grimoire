[32m+[m[32m        localStorage.setItem("menu-scroll-position", menu.scrollLeft);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    const disableSmoothScroll = '' == '1';[m
[32m+[m[32m    const enableInstantClick = '' == '1';[m
[32m+[m[41m    [m
[32m+[m[32m    if (window.matchMedia('(prefers-reduced-motion: reduce)').matches || disableSmoothScroll || enableInstantClick) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    document.querySelectorAll('a[href^="#"]').forEach(anchor => {[m
[32m+[m[32m      anchor.addEventListener("click", function (e) {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m        var id = this.getAttribute("href").substr(1);[m
[32m+[m[32m        document.querySelector(`[id='${decodeURIComponent(id)}']`).scrollIntoView({[m
[32m+[m[32m          behavior: "smooth"[m
[32m+[m[32m        });[m
[32m+[m[32m        if (id === "top") {[m
[32m+[m[32m          history.replaceState(null, null, " ");[m
[32m+[m[32m        } else {[m
[32m+[m[32m          history.pushState(null, null, `#${id}`);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  })();[m
[32m+[m[32m</script>[m
[32m+[m[32m<script>[m
[32m+[m[32m  var mybutton = document.getElementById("top-link");[m
[32m+[m[32m  window.onscroll = function () {[m
[32m+[m[32m    if (document.body.scrollTop > 800 || document.documentElement.scrollTop > 800) {[m
[32m+[m[32m      mybutton.style.visibility = "visible";[m
[32m+[m[32m      mybutton.style.opacity = "1";[m
[32m+[m[32m    } else {[m
[32m+[m[32m      mybutton.style.visibility = "hidden";[m
[32m+[m[32m      mybutton.style.opacity = "0";[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m[32m</script>[m
[32m+[m[32m<script>[m
[32m+[m[32m  document.querySelectorAll('.masked-email').forEach((el) => {[m
[32m+[m[32m    el.innerText = atob(el.innerText);[m
[32m+[m[32m    el.classList.remove('masked-email')[m
[32m+[m[32m  })[m
[32m+[m[32m</script>[m
[32m+[m[32m<script>[m
[32m+[m[32m  if (window.scrollListeners) {[m
[32m+[m[41m    [m
[32m+[m[32m    for (const listener of scrollListeners) {[m
[32m+[m[32m      window.removeEventListener('scroll', listener)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  window.scrollListeners = [][m
[32m+[m[32m</script>[m
[32m+[m
[32m+[m
[32m+[m
